%tr[entry_term]{:class => cycle("even","odd")}
  %td=h entry_term.name
  %td=h entry_term.code
  %td=h entry_term.start_date
  %td=h entry_term.end_date
  %td=h entry_term.is_default ? _('Yes') : _('No')
  %td=h entry_term.is_enabled ? _('Yes') : _('No')
  %td= render :partial => "common/actions/for_row_index_no_show", :locals => { :local => entry_term }