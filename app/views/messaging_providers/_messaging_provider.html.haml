%tr[messaging_provider]{:class => cycle("even","odd")}
  %td=h messaging_provider.name
  %td=h messaging_provider.is_default ? _('Yes') : _('No')
  %td=h messaging_provider.is_enabled ? _('Yes') : _('No')
  %td= render :partial => "common/actions/for_row_index_no_show", :locals => { :local => messaging_provider }